{
  "tracker_right_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "scroll",
          "content": {
            "type": "dock",
            "dock": "left",
            "v_alignment": "stretch",
            "margin": "0,0,3,0",
            "content": [
              {
                "type": "group",
                "header": "Items OOT",
                "dock": "top",
                "header_content": {
                  "type": "button_popup",
                  "style": "settings",
                  "popup_background": "#FF212121",
                  "layout": "options_popup"
                },
                "content": {
                  "type": "layout",
                  "key": "item_grid_tall_oot"
                }
              },
              {
                "type": "group",
                "header": "Items MM",
                "dock": "top",
                "content": {
                  "type": "layout",
                  "key": "item_grid_mini_mm"
                }
              }
            ]
          }
        },
        {
          "type": "container",
          "dock": "bottom",
          "content": {
            "type": "group",
            "header": "Pinned Locations",
            "content": {
              "type": "recentpins",
              "style": "wrap",
              "h_alignment": "stretch",
              "v_alignment": "stretch",
              "orientation": "horizontal",
              "compact": true
            }
          }
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "tabbed",
              "dock": "top",
              "tabs": [
                {
                  "title": "OOT",
                  "icon": "images/ocarina.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "oot"
                    ]
                  }
                },
                {
                  "title": "MM",
                  "icon": "images/items/mm_fiercedeity.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "termina"
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "tracker_top_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "content": [
            {
              "type": "dock",
              "dock": "left",
              "margin": "0,0,3,0",
              "content": [
                {
                  "type": "group",
                  "header": "Items OOT",
                  "dock": "top",
                  "header_content": {
                    "type": "button_popup",
                    "style": "settings",
                    "popup_background": "#FF212121",
                    "layout": "options_popup"
                  },
                  "content": {
                    "type": "layout",
                    "key": "item_grid_wide_oot"
                  }
                }
              ]
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "OOT",
              "icon": "images/ocarina.png",
              "content": {
                "type": "map",
                "maps": [
                  "oot"
                ]
              }
            },
            {
              "title": "MM",
              "icon": "images/items/mm_fiercedeity.png",
              "content": {
                "type": "map",
                "maps": [
                  "termina"
                ]
              }
            }
          ]
        }
      ]
    }
  }
}